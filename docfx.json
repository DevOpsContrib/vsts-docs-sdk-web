{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual",
        "dest": "vsts",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "api",
        "exclude": [
          "**/*..yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "src": "docs-ref-conceptual",
        "dest": "vsts",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {
      "ms.topic": {
        "docs-ref-autogen/**": "reference"
      }
    },
    "template": [],
    "dest": "vsts-docs-sdk-node"
  }
}
